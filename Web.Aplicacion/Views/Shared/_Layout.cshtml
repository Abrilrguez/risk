    <!-- notika icon CSS
        ============================================ -->
    <link rel="stylesheet" href="@Url.Content("~/Content/css/notika-custom-icon.css")">
    <!-- Ionicons CSS-->
    <link rel="stylesheet" href="@Url.Content("~/Content/css/ionicons.min.css")">
    <!-- wave CSS
        ============================================ -->
    <link rel="stylesheet" href="@Url.Content("~/Content/css/wave/waves.min.css")">


    <link rel="stylesheet" href="@Url.Content("~/Content/css/jquery.dataTables.min.css")">
    <!-- main CSS
        ============================================ -->
    <link rel="stylesheet" href="@Url.Content("~/Content/css/main.css")">
    <!-- style CSS
        ============================================ -->
    <link rel="stylesheet" href="@Url.Content("~/Content/css/style.css")">


    <!-- responsive CSS
        ============================================ -->
    <link rel="stylesheet" href="@Url.Content("~/Content/css/responsive.css")">
    <!-- modernizr JS
        ============================================ -->
    <script src="@Url.Content("~/Scripts/vendor/modernizr-2.8.3.min.js")"></script>

    <!-- datables
        ============================================ -->
    @*<link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/v/zf-6.4.3/dt-1.10.18/b-1.5.4/sl-1.2.6/datatables.min.css" />
        <script type="text/javascript" src="https://cdn.datatables.net/v/zf-6.4.3/dt-1.10.18/b-1.5.4/sl-1.2.6/datatables.min.js"></script>*@

    <!-- SweetAlert
        ============================================ -->
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>

    <script type="text/javascript">
        var baseUrl = "@Url.Content("~")";
    </script>

    <style>
        .nav-tabs > li.active > a {
            color: #fff;
            background-color: var(--color-principal);
        }

            .nav-tabs > li.active > a:hover {
                color: #fff;
                background-color: #d32e5e;
            }

        .nav-tabs > li > a {
            border-radius: 4px;
        }

        .mean-container .mean-nav ul li a {
            text-transform: capitalize;
        }
    </style>
</head>

<body>

    <!-- Start Header Top Area -->
    <div class="header-top-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                    <div class="logo-area">
                        <a href="#"><img src="@Url.Content("~/Content/assets/img/logo/Logo-5.png")" alt="" /></a>
                    </div>
                </div>
                <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                    <div class="header-top-menu">
                        <ul class="nav navbar-nav notika-top-nav">
                            <li class="nav-item dropdown">
                                <a href="#" data-toggle="dropdown" role="button" aria-expanded="false" class="nav-link dropdown-toggle">@Session["usuarioNombre"]
                                    <i class="icon ion-md-arrow-dropdown" style="padding-left:5px"></i></a>
                                <div role="menu" class="dropdown-menu message-dd animated zoomIn">
                                    <div class="hd-message-info">
                                        <a href="@Url.Action("Login", "Home")">
                                            <div class="hd-message-sn">
                                                <div class="hd-mg-ctn">
                                                    <h3 style="display: flex;justify-content: space-between;">Cerrar sesion<i class="icon ion-md-log-out"></i></h3>
                                                </div>
                                            </div>
                                        </a>
                                    </div>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- End Header Top Area -->
    <!-- Mobile Menu start -->
    <div class="mobile-menu-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="mobile-menu" style="text-transform:capitalize;">
                        <nav id="dropdown">
                            <ul class="mobile-menu-nav">
                                @if (Convert.ToInt32(Session["usuarioId"]) != 0)
                                {

                                    <li @if (ViewBag.Title == "Orden") { <text> class="active" </text> }>
                                        <a href="@Url.Action("Index", "Orden")"><i class="icon ion-md-paper"></i> Orden</a>
                                    </li>

                                    <li @if (ViewBag.Title == "Usuarios") { <text> class="active" </text> }
                                        @if (Session["usuarioRol"].ToString() == "EMPLEADO") { <text> style="display:none" </text> }>
                                        <a href="@Url.Action("Index", "Usuario")"><i class="icon ion-md-person"></i> Usuarios</a>
                                    </li>

                                    <li @if (ViewBag.Title == "Clientes") { <text> class="active" </text> }
                                        @if (Session["usuarioRol"].ToString() == "EMPLEADO") { <text> style="display:none" </text> }>
                                        <a href="@Url.Action("Index", "Cliente")"><i class="icon ion-md-people"></i> Clientes</a>
                                    </li>
                                    <li @if (ViewBag.Title == "Destinatarios") { <text> class="active" </text> }
                                        @if (Session["usuarioRol"].ToString() == "EMPLEADO") { <text> style="display:none" </text> }>
                                        <a href="@Url.Action("Index", "Destinatario")"><i class="icon ion-md-mail-open"></i> Destinatarios</a>
                                    </li>
                                    <li @if (ViewBag.Title == "Historial") { <text> class="active" </text> }>
                                        <a href="@Url.Action("Index", "Historial")"><i class="icon ion-md-calendar"></i> Historial</a>
                                    </li>
                                }
                            </ul>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Mobile Menu end -->
    <!-- Main Menu area start-->
    <div class="main-menu-area mg-tb-40">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <ul class="nav nav-tabs notika-menu-wrap menu-it-icon-pro">
                        @if (Convert.ToInt32(Session["usuarioId"]) != 0)
                        {

                            <li @if (ViewBag.Title == "Orden") { <text> class="active" </text>    }>
                                <a href="@Url.Action("Index", "Orden")"><i class="icon ion-md-paper"></i> Orden</a>
                            </li>

                            <li @if (ViewBag.Title == "Usuarios") { <text> class="active" </text>    }
                                @if (Session["usuarioRol"].ToString() == "EMPLEADO") { <text> style="display:none" </text>  }>
                                <a href="@Url.Action("Index", "Usuario")"><i class="icon ion-md-person"></i> Usuarios</a>
                            </li>

                            <li @if (ViewBag.Title == "Clientes") { <text> class="active" </text>   }
                                @if (Session["usuarioRol"].ToString() == "EMPLEADO") { <text> style="display:none" </text>  }>
                                <a href="@Url.Action("Index", "Cliente")"><i class="icon ion-md-people"></i> Clientes</a>
                            </li>
                            <li @if (ViewBag.Title == "Destinatarios") { <text> class="active" </text>  }
                                @if (Session["usuarioRol"].ToString() == "EMPLEADO") { <text> style="display:none" </text>  }>
                                <a href="@Url.Action("Index", "Destinatario")"><i class="icon ion-md-mail-open"></i> Destinatarios</a>
                            </li>
                            <li @if (ViewBag.Title == "Historial") { <text> class="active" </text>   }>
                                <a href="@Url.Action("Index", "Historial")"><i class="icon ion-md-calendar"></i> Historial</a>
                            </li>
                        }
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <!-- Main Menu area End-->

    @RenderBody()



    <!-- bootstrap JS
        ============================================ -->
    <script src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>
    <!-- wow JS
        ============================================ -->
    <script src="@Url.Content("~/Scripts/wow.min.js")"></script>
    <!-- price-slider JS
        ============================================ -->
    <script src="@Url.Content("~/Scripts/jquery-price-slider.js")"></script>
    <!-- owl.carousel JS
        ============================================ -->
    <script src="@Url.Content("~/Scripts/owl.carousel.min.js")"></script>
    <!-- scrollUp JS
        ============================================ -->
    <script src="@Url.Content("~/Scripts/jquery.scrollUp.min.js")"></script>
    <!-- meanmenu JS
        ============================================ -->
    <script src="@Url.Content("~/Scripts/meanmenu/jquery.meanmenu.js")"></script>
    <!-- counterup JS
        ============================================ -->
    <script src="@Url.Content("~/Scripts/counterup/jquery.counterup.min.js")"></script>
    <script src="@Url.Content("~/Scripts/counterup/waypoints.min.js")"></script>
    <script src="@Url.Content("~/Scripts/counterup/counterup-active.js")"></script>
    <!-- mCustomScrollbar JS
        ============================================ -->
    <script src="@Url.Content("~/Scripts/scrollbar/jquery.mCustomScrollbar.concat.min.js")"></script>
    <!-- jvectormap JS
        ============================================ -->
    <script src="@Url.Content("~/Scripts/jvectormap/jquery-jvectormap-2.0.2.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jvectormap/jquery-jvectormap-world-mill-en.js")"></script>
    <script src="@Url.Content("~/Scripts/jvectormap/jvectormap-active.js")"></script>
    <!-- sparkline JS
        ============================================ -->
    <script src="@Url.Content("~/Scripts/sparkline/jquery.sparkline.min.js")"></script>
    <script src="@Url.Content("~/Scripts/sparkline/sparkline-active.js")"></script>
    <!-- sparkline JS
        ============================================ -->
    <script src="@Url.Content("~/Scripts/flot/jquery.flot.js")"></script>
    <script src="@Url.Content("~/Scripts/flot/jquery.flot.resize.js")"></script>
    <script src="@Url.Content("~/Scripts/flot/curvedLines.js")"></script>
    <script src="@Url.Content("~/Scripts/flot/flot-active.js")"></script>
    <!-- knob JS
        ============================================ -->
    <script src="@Url.Content("~/Scripts/knob/jquery.knob.js")"></script>
    <script src="@Url.Content("~/Scripts/knob/jquery.appear.js")"></script>
    <script src="@Url.Content("~/Scripts/knob/knob-active.js")"></script>
    <!--  wave JS
        ============================================ -->
    <script src="@Url.Content("~/Scripts/wave/waves.min.js")"></script>
    <script src="@Url.Content("~/Scripts/wave/wave-active.js")"></script>
    <!--  todo JS
        ============================================ -->
    <script src="@Url.Content("~/Scripts/todo/jquery.todo.js")"></script>
    <!-- plugins JS
        ============================================ -->
    <script src="@Url.Content("~/Scripts/plugins.js")"></script>
    <!--  Chat JS
        ============================================ -->
    <script src="@Url.Content("~/Scripts/chat/moment.min.js")"></script>
    <script src="@Url.Content("~/Scripts/chat/jquery.chat.js")"></script>
    <!-- main JS
        ============================================ -->
    <script src="@Url.Content("~/Scripts/main.js")"></script>
    <!-- tawk chat JS
        ============================================ -->
    @*<script src="@Url.Content("~/Scripts/tawk-chat.js")"></script>*@

    <!-- Input Mask JS
        ============================================ -->
    <script src="@Url.Content("~/Scripts/jasny-bootstrap.min.js")"></script>
    <!-- autosize JS
        ============================================ -->
    <script src="@Url.Content("~/Scripts/autosize.min.js")"></script>
</body>

</html>
